<!DOCTYPE HTML>
<html>
<head>
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <script>
window.onload = function () {
var chart1 = new CanvasJS.Chart("chartContainer", {
	animationEnabled: true,
	title: {
		text: "Status programari"
	},
	data: [{
		type: "doughnut",
		startAngle: 60,
		indexLabelFontSize: 16,
		indexLabel: "{suprafata} - {valoare}%",
		toolTipContent: "<b>{suprafata}:</b> {valoare}%",
		dataPoints: @Html.Raw(ViewBag.DataPointsTasks)}]
});
chart1.render();
}
    </script>
</head>
<body>
    <h2>Grafice</h2>
    <div id="chartContainer" style="height: 370px; padding:25px 25px 25px 25px"></div>
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</body>
</html>

