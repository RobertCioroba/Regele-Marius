@model Regele_Marius.Models.RezultatAnaliza

@{
    ViewBag.Title = "Buletin analize";
}

<div style="text-align:right">@Html.ActionLink("Export in PDF", "GeneratePDF", new { id = Model.Id })</div>

<div>
