                <span>@Html.DisplayFor(model => model.Pacient.Email)</span>
            </div>
            <div class="detaliu-pacient">
                <span>@Html.DisplayNameFor(model => model.DataNastere)</span>
                <span>@Html.DisplayFor(model => model.DataNastere)</span>
            </div>
            <div class="detaliu-pacient">
                <span>@Html.DisplayNameFor(model => model.NrTelefon)</span>
                <span>@Html.DisplayFor(model => model.NrTelefon)</span>
            </div>
            <div class="detaliu-pacient">
                <span>@Html.DisplayNameFor(model => model.Adresa)</span>
                <span>@Html.DisplayFor(model => model.Adresa)</span>
            </div>
            <div class="detaliu-pacient">
