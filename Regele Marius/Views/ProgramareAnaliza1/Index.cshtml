@model IEnumerable<Regele_Marius.Models.ProgramareAnaliza>

@{
    ViewBag.Title = "Lista programari analiza";

    if (Session["NumeUtilizator"] == null)
    {
        Response.Redirect("~/Cont/Logare", false);
    }
}

<h2>Lista programari analiza</h2>

<p>
    @Html.ActionLink("Programare noua", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Analiza.Denumire)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Medic.Nume)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PacientId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataProgramare)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OraInceput)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Analiza.Denumire)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Medic.Nume)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PacientId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataProgramare)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OraInceput)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

